{{!
Render hz-*version
}}

{{#if [x-hz-version]}}
    <span class="hz-property-version">v{{[x-hz-version]}}</span>
{{/if}}
{{#if [x-hz-minimumVersion]}}
    <span class="hz-property-version">min-v{{[x-hz-minimumVersion]}}</span>
{{/if}}
{{#if [x-hz-maximumVersion]}}
    <span class="hz-property-version">max-v{{[x-hz-maximumVersion]}}</span>
{{/if}}
